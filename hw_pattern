int z=30;


void setup()  {
  size(800,500);
  
}

void draw()  {
   background(#B2ECFC);
  for(int y = 0; y <= height; y = y + 70)  {
    for(int x = 0; x <= width; x += 70) {
      
stroke(#FA9FE9);
strokeWeight(2);
ellipse(x,y,30,30);
ellipse(x,y,5,5);

    }
  }
 stroke(#FA44D9);
z=z+2;
ellipse(z,30,15,15);
ellipse(z,100,15,15);
ellipse(z,170,15,15);
ellipse(z,240,15,15);
ellipse(z,310,15,15);
ellipse(z,380,15,15);
ellipse(z,450,15,15);
if((z>width)|| (z<0)) {
z=-2;
}

}
